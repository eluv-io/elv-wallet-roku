<?xml version="1.0" encoding="utf-8"?>
<component name="TokenStore" extends="Node">
    <interface>
        <field id="fabricToken" type="string" />
        <field id="tokenExpiration" type="string" />
        <field id="refreshToken" type="string" />
        <field id="clusterToken" type="string" />
        <field id="walletAddress" type="string" />
        <field id="userEmail" type="string" />
        <!-- The login provider the current fabricToken was obtained for -->
        <field id="loginProvider" type="string" />

        <!-- Clear all data in the token store. Effectively a "sign out" operation. -->
        <function name="wipe" />
        <function name="GetActivationData" />
        <function name="checkDeviceAuthorization" />
        <function name="passwordAuth" />
        <function name="refreshTokens" />
    </interface>
</component>