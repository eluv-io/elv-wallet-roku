<?xml version="1.0" encoding="UTF-8"?>
<component name="PropertyStore" extends="Node">
    <interface>
        <!-- Array of all Properties known to the app -->
        <field id="properties" type="array" value="[]" />
        <!-- A map of PropertyID_PageID to list of sections in the papge -->
        <field id="sections" type="assocArray" value="{}" />

        <function name="fetchDiscoverableProperties" />
        <function name="getPropertyById" />
        <function name="fetchSections" />
    </interface>
</component>