import "pkg:/components/utils/RouterUtils.bs"

sub Init()
    signinButton = m.top.findNode("btn_signin")
    signinButton.ObserveField("buttonSelected", "onSigninClicked")
    m.top.findNode("foo").ObserveField("buttonSelected", "onsigninclicked")
end sub

sub OnSigninClicked()
    router()@.show({ view: CreateObject("roSGNode", "SignIn") })
end sub