<?xml version="1.0" encoding="UTF-8"?>
<component name="NftDetail" extends="Group">
    <interface>
        <field id="nft" type="node" onChange="onNftItemChanged" />
    </interface>
    <children>
        <Rectangle color="#000000" width="1920" height="1080">
            <LayoutGroup layoutDirection="vert" translation="[80, 80]">
                <TvLabel
                    id="title"
                    numLines="2"
                    width="1000"
                    wrap="true"
                    maxLines="2"
                />
                <MultiStyleLabel
                    id="subtitle"
                    maxLines="5"
                    wrap="true"
                    width="1300"
                />
                <RowList id="rowlist" />
            </LayoutGroup>
        </Rectangle>
    </children>
</component>