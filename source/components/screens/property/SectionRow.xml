<?xml version="1.0" encoding="UTF-8"?>
<component name="SectionRow" extends="Group">
    <interface>
        <field id="rowContent" type="node" onChange="onSectionChanged" />
        <field id="width" type="float" />
        <field id="height" type="float" />
        <field id="drawBeforeItems" type="boolean" value="true" />
        <field id="rowFocusPercent" type="float" onChange="onRowFocusChange" />
    </interface>
    <children>
        <Spacer id="bgColor" width="1920" />
        <Poster id="bgImage" width="1920" />
        <TvLabel id="title" fontStyle="body_32" />
    </children>
</component>