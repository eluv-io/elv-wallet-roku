<?xml version="1.0" encoding="utf-8"?>
<component name="SignIn" extends="BaseView" initialFocus="btn_new_code">
    <interface>
        <!-- Must be set before [property]!! -->
        <field id="useMetamask" type="boolean" value="false" />
        <field id="property" type="assocArray" onChange="onPropertyChanged" />
    </interface>
    <children>
        <Timer id="code_expiration_timer" repeat="false" />
        <Timer id="status_poll_timer" repeat="true" />
    </children>
</component>