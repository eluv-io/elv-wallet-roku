<?xml version="1.0" encoding="utf-8"?>
<component name="TabItem" extends="Group">

    <interface>
        <field id="itemContent" type="node" onChange="onItemContentChanged" />
        <field id="itemHasFocus" type="boolean" onChange="onItemFocusChanged" />
        <field id="focusPercent" type="float" onChange="onFocusChanged" />
        <field id="gridHasFocus" type="boolean" onChange="onFocusChanged" />
        <field id="width" type="float" onChange="onSizeChange" />
        <field id="height" type="float" onChange="onSizeChange" />
    </interface>

    <children>
        <SimpleLabel id="label" horizOrigin="center" vertOrigin="center" />
    </children>
</component>