<?xml version="1.0" encoding="utf-8"?>
<component name="TabItem" extends="Group">

    <interface>
        <field id="itemContent" type="node" onChange="onItemContentChanged" />
        <field id="itemHasFocus" type="boolean" onChange="onItemFocusChanged" />
        <field id="focusPercent" type="float" onChange="onFocusPercentChanged" />
    </interface>

    <children>
        <Label id="label" translation="[10, 10]" />
    </children>
</component>