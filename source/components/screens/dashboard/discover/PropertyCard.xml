<?xml version="1.0" encoding="UTF-8"?>
<component name="PropertyCard" extends="Group">
    <interface>
        <field id="itemContent" type="node" onChange="onContentChanged" />
        <field id="width" type="float" onChange="onWidthChanged" />
        <field id="height" type="float" onChange="onHeightChanged" />
    </interface>
    <children>
        <Poster id="image" loadDisplayMode="scaleToZoom" width="330" height="468">
            <Shimmer id="image_shimmer" control="start" />
        </Poster>
    </children>
</component>