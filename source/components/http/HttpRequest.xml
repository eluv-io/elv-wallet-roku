<?xml version="1.0" encoding="UTF-8"?>
<component name="HttpRequest" extends="Node">
    <interface>
        <field id="url" type="string" />
        <field id="method" type="string" value="GET" />
        <field id="body" type="string" />
        <!-- Responses that are too big can mess up the console -->
        <field id="printResponseBody" type="boolean" value="false" />

        <!-- Auth0 will regularly give 403 while activation is pending -->
        <field id="skipAuthHandling" type="boolean" value="false" />
    </interface>
</component>